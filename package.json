{
  "name": "zenith",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- vite",
    "start:backend": "pnpm --filter ./app/backend start",
    "start:dev": "concurrently \"pnpm start:backend\" \"pnpm run dev\" --names \"backend,frontend\" --prefix-colors \"cyan,magenta\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.47.5",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "from": "^0.1.7",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "motion": "^12.20.1",
    "nodemailer": "^7.0.5",
    "ol": "^10.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router": "^7.6.3",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.0.10",
